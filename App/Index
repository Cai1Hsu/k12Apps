    <script>

        $(function () {
                        //返回门户
            $('#logo_Res').click(function () {
                window.location.href = '/Login/BackToPortal';
            })
            $('#backToPortal').click(function () {
                window.location.href = '/Login/BackToPortal';
            })
            $("#loginOut").click(function () {
                window.location.href ='/Login/LoginOut';
            });

            var urlmap = [
                {
                    src: "/ExamPlan/Exam/Othre",
                    dest: "/ExamPlan/Exam/Index"
                },
                {
                    src: "/ExamPlan/Exam/ExamSetting",
                    dest: "/ExamPlan/Exam/Index"
                }
            ];
            var path = window.location.pathname;

            $(urlmap).each(function () {
                var src = this.src;
                var dest = this.dest;
                if (src == path) {
                    path = dest;
                }
            });

            var activeNav = null;
            if (path == "/") {
                activeNav = $(".layui-nav [default-page]");
            } else {
                var navList = $(".layui-nav a[href]");
                $(navList).each(function () {
                    var currNav = this;
                    var url = $(currNav).attr("href");
                    if (url.indexOf(path) >= 0) {
                        activeNav = $(currNav);
                    }
                });
            }
            if (activeNav != null && activeNav != undefined) {
                activeNav.addClass("layui-this");
                var pNode = activeNav.parents(".layui-nav-item");
                if (pNode != null && pNode != undefined) {
                    pNode.addClass("layui-nav-itemed");
                }
            }
            layui.config({
                base: '/Plugin/TreeTable/js/'
            });
            //JavaScript代码区域
            layui.use(['element', 'form', 'layer','treetable'], function () {
                var element = layui.element;
                var layer = layui.layer;
                var treetable = layui.treetable;
                //鼠标悬浮显示帮助说明
                $('.icon_tip').mouseover(function () {
                    var content = $(this).find('.tip_content').text()
                    var index = layer.tips(content, '.icon_tip', {
                        time:0
                    });
                    $(this).mouseout(function () {
                        layer.close(index)
                    })
                })

                //数据加载区域
                $("#addApp").click(function() {
                    layer.open({
                        type: 2,
                        title: "新增应用",
                        content: "/App/AddApp",
                        offset: 'auto',
                        area: ["420px", "470px"],
                        maxmin: true,
                        end: function() {
                            window.location.reload();
                        }
                    });
                });

                //分组操作部分开始
                $("#addAppGroup").click(function() {
                    layer.open({
                        type: 2,
                        title: "新增应用",
                        content: "/App/AddAppGroup",
                        offset: 'auto',
                        area: ["420px", "470px"],
                        maxmin: true,
                        end: function() {
                            window.location.reload();
                        }
                    });
                });
                $("li a[gdelete]").click(function() {
                        var id = $(this).attr("attr");
                        layer.confirm('确认删除该应用?',
                            function(index) {
                                var load = layer.msg("应用删除中...", { icon: 16, shade: 0.01, time: 0 });
                                $.ajax({
                                    type: "POST",
                                    dataType: "json",
                                    url: "/App/DeleteAppGroup",
                                    data: { id: id },
                                    success: function(data) {
                                        layer.close(index);
                                        layer.close(load);
                                        if (data.flag == 0) {
                                            layer.msg(data.message, { icon: 7 });
                                        } else {
                                            layer.msg("删除成功",
                                                { icon: 1, time: 1000 },
                                                function() {
                                                    window.location.reload();
                                                });
                                        }
                                    },
                                    error: function(msg) {
                                        layer.msg("删除异常", { icon: 7 });
                                    }
                                });
                            });
                    });

                $("li a[gupdate]").click(function() {
                    var id = $(this).attr("attr");
                    layer.open({
                        type: 2,
                        title: "修改应用",
                        content: "/App/AddAppGroup?id=" + id,
                        offset: 'auto',
                        area: ["420px", "490px"],
                        maxmin: true,
                        end: function() {
                            window.location.reload();
                        }
                    });
                });

                $("li a[gsearch]").click(function() {
                    loadhtmlg(this);
                });

                window.loadhtmlg = function(obj) {
                     //选中
                    $("li a[gsearch]").each(function() {
                        $(this).removeClass("activeLi");
                    });

                    $(obj).addClass("activeLi");

                    var id = $(obj).attr("attr");
                    //加载数据
                    $.ajax({
                        type: "GET",
                        dataType: "html",
                        url: "/App/QueryApp?AppGroupId=" + id,
                        success: function (result) {
                            $("#ul_app").empty();
                            JSON.parse(result).forEach(item => {
                                var dom = '<li class="li-bottom" lay-id="doc" style="position:relative;"><a class="a-style" search attr="' + item.AppId + '" style=" padding: 5px;"><i class="iconfont icon-wendang"></i>' + item.AppName + (item.AppTag == null || item.AppTag.length==0 ?'': '[' + item.AppTag + ']' )+ '</a>';
                            if (item.IsMobile)
                            {
                                dom += '<span class="app_mobile">app</span>';
                            }
                            else
                            {
                                dom += '<span class="app_pc">pc</span>';
                            }
                                dom += ' <a update class="a-style" attr="'+item.AppId+'" style="position:absolute;right:40px;top:8px;">修改</a>';
                                dom += '<a delete class="a-style" attr="'+item.AppId+'" style="position:absolute;right:0px;top:8px;">删除</a>';
                                dom += '</li>'
                                $("#ul_app").append(dom);
                            });

                $("li a[delete]").click(function() {
                        var id = $(this).attr("attr");
                        layer.confirm('确认删除该应用?',
                            function(index) {
                                var load = layer.msg("应用删除中...", { icon: 16, shade: 0.01, time: 0 });
                                $.ajax({
                                    type: "POST",
                                    dataType: "json",
                                    url: "/App/DeleteApp",
                                    data: { id: id },
                                    success: function(data) {
                                        layer.close(index);
                                        layer.close(load);
                                        if (data.flag == 0) {
                                            layer.msg(data.message, { icon: 7 });
                                        } else {
                                            layer.msg("删除成功",
                                                { icon: 1, time: 1000 },
                                                function() {
                                                    window.location.reload();
                                                });
                                        }
                                    },
                                    error: function(msg) {
                                        layer.msg("删除异常", { icon: 7 });
                                    }
                                });
                            });
                    });

                $("li a[update]").click(function() {
                    var id = $(this).attr("attr");
                    layer.open({
                        type: 2,
                        title: "修改应用",
                        content: "/App/UpdateApp?id=" + id,
                        offset: 'auto',
                        area: ["420px", "490px"],
                        maxmin: true,
                        end: function() {
                            window.location.reload();
                        }
                    });
                });

                $("li a[search]").click(function() {
                    loadhtml(this);
                });

                        },
                        error: function(msg) {
                            //layer.msg("删除异常", { icon: 7 });
                        }
                    });
                }

                //分组操作部分结束

                window.loadhtml = function(obj) {
                     //选中
                    $("li a[search]").each(function() {
                        $(this).removeClass("activeLi");
                    });

                    $(obj).addClass("activeLi");

                    var id = $(obj).attr("attr");
                    $("#CurrentAppId").val(id);
                        //加载数据
                    $.ajax({
                        type: "GET",
                        dataType: "html",
                        url: "/App/QueryMenuList?appId=" + id,
                        success: function(result) {

                            $("#div_content").html(result);
                        },
                        error: function(msg) {
                            //layer.msg("删除异常", { icon: 7 });
                        }
                    });
                }
                $("#btnAddMenu").click(function() {
                    var currId = $("#CurrentAppId").val();
                    if (currId == "") {
                        layer.msg("当前未选择应用,不能添加菜单", {icon:7});
                        return false;
                    }

                     layer.open({
                        type: 2,
                        title: "新增菜单",
                        content: "/App/AddMenu?appId=" + currId,
                        offset: 'auto',
                        area: ["440px", "550px"],
                        maxmin: true,
                        end: function () {
                            var obj = $("a[attr='" + currId + "'][search]");
                            loadhtml(obj);
                        }
                    });
                });

            });


        });


        function post(URL, AppId, WebApi, WebApiLogin, RevertAppId) {
            //returnMain(URL, AppId, WebApiLogin, RevertAppId);
            $.ajax({
                headers: {
                    "appid": AppId,
                    "access_token": JSON.parse($.cookie('token')).access_token,
                    "refresh_token": JSON.parse($.cookie('token')).refresh_token,
                    "targetId": RevertAppId
                },
                type: "post",
                async: false,
                url: WebApi + "/account/SwitchApp",
                data: {},
                dataType: "json",
                success: function (data) {
                    if (data.HasError) {
                        $.ajax({
                            headers: { "refresh_token": refresh_token, "AppId": appId },
                            type: "post",
                            url: WebApi + "/account/RefreshAccessToken",
                            data: null,
                            dataType: "json",
                            success: function (data) {
                                if (data.HasError) {
                                    alert(data.Message);
                                    window.location.href = WebApiLogin;
                                } else {
                                    var token = { access_token: data.Data.access_token, refresh_token: data.Data.refresh_token }
                                    $.cookie('token', JSON.stringify(token));
                                }
                            }
                        });
                    } else {
                        res_access_token = data.Data.access_token;
                        res_refresh_token = data.Data.refresh_token;

                        var temp = document.createElement("form");
                        temp.action = URL;
                        temp.method = "post";
                        temp.style.display = "none";
                        var access_token = document.createElement("textarea");
                        var refresh_token = document.createElement("textarea");
                        var appList = document.createElement("textarea");
                        var user = document.createElement("textarea");
                        access_token.name = "access_token";
                        access_token.value = res_access_token;
                        refresh_token.name = "refresh_token";
                        refresh_token.value = res_refresh_token;
                        appList.name = "appList";
                        appList.value = "";
                        user.name = "user";
                        user.value = "";

                        temp.appendChild(access_token);
                        temp.appendChild(refresh_token);
                        temp.appendChild(appList);
                        temp.appendChild(user);
                        document.body.appendChild(temp);
                        temp.submit(); return temp;
                    }
                }
            })
        }


    </script>
</body>
</html>